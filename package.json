{
  "name": "jota",
  "version": "1.0.0",
  "description": "App to read and search Bible",
  "productName": "Jota",
  "cordovaId": "com.github.jota-bible.jota-app",
  "capacitorId": "",
  "author": "Jacek Ko≈Çodziejczyk <jacek.p.kolodziejczyk@gmail.com>",
  "private": false,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:unit:clear": "jest --clearCache",
    "custom-icons": "fantasticon src/custom-icons/svg -o src/statics/custom-icons --normalize -t woff woff2 -g css",
    "custom-icons:watch": "watch 'npm run custom-icons' src/custom-icons/svg"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "node-sass": "^4.14.1",
    "quasar": "^1.15.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@quasar/app": "^1.8.10",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.1.2",
    "fantasticon": "^2.0.0"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "@babel/parser": "7.7.5"
  }
}
